<configuration>
  <system.webServer>

    <!-- indicates that the hello.js file is a node.js application
    to be handled by the iisnode module -->

    <handlers>
      <add name="iisnode" path="within.js" verb="*" modules="iisnode" />
    </handlers>

    <rewrite>
      <rules>
        <rule name="t1">
          <match url="/*" />
          <action type="Rewrite" url="within.js" />
        </rule>
      </rules>
    </rewrite>

    <iisnode nodeProcessCommandLine="&quot;\utils\nodejs\node.exe&quot;" />
  </system.webServer>
</configuration>
